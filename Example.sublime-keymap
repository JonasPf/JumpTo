    { "keys": ["ctrl+shift+e"], "command": "jump_to_interactive", "args": { "extend": true, "whole_match": true } },
    { "keys": ["alt+e"], "command": "jump_to_interactive", "args": { "create_new": true } },
    { "keys": ["ctrl+alt+e"], "command": "jump_to_interactive", "args": { "create_new": true, "whole_match": true } }
]
